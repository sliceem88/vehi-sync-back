{
  "name": "vehi-sync-back",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "debug": "adonis serve --dev --debug",
    "test": "node ace test",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#enums/*": "./app/enums/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#utilities/*": "./app/utilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/tsconfig": "^1.4.1",
    "@japa/api-client": "^3.2.1",
    "@japa/assert": "^4.2.0",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.5.0",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@swc/core": "1.15.11",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.1.3",
    "ts-node-maintained": "^10.9.6",
    "typescript": "~5.9"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.6.0",
    "@adonisjs/core": "^6.20.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.8.2",
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@vinejs/vine": "^4.3.0",
    "adonis-lucid-soft-deletes": "^2.1.0",
    "aws-sdk": "^2.1693.0",
    "http-status-codes": "^2.3.0",
    "luxon": "^3.7.2",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  }
}
